<% provide(:title, 'Sign up') %>
<% provide(:button_text, 'Add a staff member') %>
<h1>Add staff member</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@staff) do |f| %>

      <%= f.label :dental_number %>
      <%= f.text_field :dental_number, class: 'form-control' %>

      <%= f.submit yield(:button_text), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>