<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
  <li>
    <%= gravatar_for user, size: 50 %>
    <%= link_to user.first_name, user %>
    | <%= link_to "delete", user, method: :delete,
                  data: {confirm: "You sure?"} %>
  </li>
</ul>

<%= will_paginate %>